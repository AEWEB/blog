シード
 
use arcadia_on_staging;
SELECT kit_id,data_genes.name,GHnumber,value FROM  ((kit_unit_genes inner join kit_units on kit_unit_genes.kit_unit_id=kit_units.id) 
	inner join data_genes on  kit_unit_genes.data_gene_id=data_genes.id)
	inner join data_alleles on kit_unit_genes.data_allele_id=data_alleles.id
where kit_unit_genes.data_unit_id="307" and kit_unit_genes.data_gene_id ="971" 

 
